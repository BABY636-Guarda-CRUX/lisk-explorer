{
  "name": "lisk-explorer",
  "version": "3.0.0-beta.0",
  "description": "Lisk Explorer",
  "keywords": [
    "lisk",
    "explorer",
    "blockchain"
  ],
  "homepage": "https://github.com/LiskHQ/lisk-explorer",
  "repository": {
    "type": "git",
    "url": "git@github.com:LiskHQ/lisk-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/LiskHQ/lisk-explorer/issues"
  },
  "private": true,
  "author": "Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
  "license": "GPL-3.0",
  "main": "app.js",
  "engines": {
    "node": "8.11.2",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "npm run build && node app.js",
    "test": "npm run test:e2e",
    "test:e2e": "protractor protractor.conf.js",
    "test:e2e:install": "./node_modules/protractor/bin/webdriver-manager update --standalone false --gecko false",
    "eslint": "eslint ./src ./features ./*.js ./*.json",
    "eslint-fix": "eslint --fix ./src ./features ./*.js",
    "watch": "npm run build:copy-files && DEBUG=true webpack -w",
    "validate": "webpack-validator webpack.config.js",
    "build": "npm run build:copy-files && webpack",
    "build:prod": "webpack --env.prod",
    "build:copy-files": "cp -r ./src/assets/img/favicons/* ./src/assets/img/leaflet ./robots.txt ./public",
    "clean": " npm run clean-frontend && git clean ./ -dfnx",
    "clean:frontend": "git clean ./public -dfx && git clean ./src -dfx"
  },
  "dependencies": {
    "amstock3": "github:amcharts/amstock3",
    "angular": "<=1.6.10",
    "angular-animate": "~1.6.4",
    "angular-cookies": "~1.6.10",
    "angular-gettext": "^2.3.9",
    "angular-naturalsort": "https://github.com/isabello/angular-naturalsort.git",
    "angular-qrcode": "^7.2.0",
    "angular-resource": "~1.6.4",
    "angular-sanitize": "~1.6.10",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "=1.0.0-rc.1",
    "bootstrap": "^3.3.7",
    "clipboard": "=1.7.1",
    "commander": "=2.9.0",
    "compression": "^1.6.2",
    "express": "^4.16.3",
    "fast-safe-stringify": "^2.0.6",
    "flatstr": "^1.0.12",
    "font-awesome": "^4.7.0",
    "http-proxy-middleware": "^0.19.1",
    "jquery": "^3.4.1",
    "leaflet": "~1.0.3",
    "leaflet.markercluster": "^1.3.0",
    "method-override": "^2.3.4",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ng-annotate": "^1.2.1",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "ngprogress": "https://github.com/VictorBjelkholm/ngProgress.git",
    "parse-markdown-js": "^1.3.6",
    "qrcode": "^0.8.1",
    "qrcode-generator": "^1.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sigma": "https://github.com/goVanilla/sigma.js.git",
    "socket.io-client": "^2.2.0",
    "split": "^1.0.0",
    "string-similarity": "^1.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "=7.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.11",
    "cucumber": "=2.3.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-lisk-base": "^0.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-protractor": "^1.40.0",
    "eslint-plugin-react": "^7.10.0",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.6.0",
    "imports-loader": "^0.7.1",
    "mocha": "^5.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "optipng-bin": "^4.0.0",
    "pngquant-bin": "^4.0.0",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "=3.1.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "should": "^11.2.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0",
    "style-loader": "^0.16.1",
